   "source": [
    "fml = \"suicides_no ~ C(year) + C(sex) + C(age) + gdp_per_capita\"\n",
    "model = sm.GLM.from_formula(fml, offset=np.log(df['population']), data=df, family=sm.families.Poisson())\n",
    "res = model.fit()\n",
    "print(res.summary())"
